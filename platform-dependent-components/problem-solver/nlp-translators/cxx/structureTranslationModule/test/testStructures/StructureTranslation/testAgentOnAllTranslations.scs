test_structure = [*
    concept_red <- color;;
    concept_apple <- concept_fruit;;
    concept_apple <= nrel_decomposition:
    {
       [кожура] (* <- lang_ru;; *);
       [мякоть] (* <- lang_ru;; *)
    };;
    concept_apple => nrel_name: [Арнольд] (* <- lang_ru;; *);;
    concept_man => nrel_likes: nrel_likes_to_eat: pizza;;
    concept_thing <= nrel_parts_decomposition:
    {
        part1;
        part2
    };;
    concept_man => nrel_synonyms:
    {
       [хомо сапиенс] (* <- lang_ru;; *);
       [людь] (* <- lang_ru;; *)
    };;
    concept_human => nrel_parents:
    {
        parent1;
        parent2
    };;
    *];;

